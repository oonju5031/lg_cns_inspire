<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.am_inspire_spring.user.dao.UserMapper">
    <insert id="insertRow" parameterType="com.example.am_inspire_spring.user.domain.dto.UserRequestDTO">
        INSERT INTO user_tbl (email, password, name)
        VALUES (#{email}, #{password}, #{name})
    </insert>
</mapper>